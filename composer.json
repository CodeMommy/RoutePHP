{
    "name": "codemommy/routephp",
    "type": "library",
    "license": "Apache-2.0",
    "homepage": "http://www.codemommy.com",
    "description": "Route for MVC framework",
    "keywords": [
        "CodeMommy",
        "RoutePHP",
        "Route",
        "PHP"
    ],
    "support": {
        "issues": "https://github.com/CodeMommy/RoutePHP/issues",
        "source": "https://github.com/CodeMommy/RoutePHP"
    },
    "authors": [
        {
            "name": "Candison November",
            "role": "Founder",
            "email": "kandisheng@163.com",
            "homepage": "http://www.kandisheng.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMommy\\RoutePHP\\": [
                "class",
                "interface"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMommy\\RoutePHP\\Test\\": "test",
            "CodeMommy\\RoutePHP\\Script\\": "script",
            "Controller\\": "test_case/controller",
            "ControllerNoNamespaceRoot\\": "test_case"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/http-foundation": "2.8.*",
        "symfony/routing": "2.8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "<7.0.0",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*",
        "satooshi/php-coveralls": "*",
        "CodeMommy/TaskPHP": "*"
    },
    "scripts": {
        "environment": [
            "composer -V",
            "php -v",
            "php -m"
        ],
        "update-project": [
            "git pull",
            "composer self-update",
            "composer update"
        ],
        "test": [
            "composer phpunit",
            "composer phpcbf",
            "composer phpcs",
            "composer phpmd"
        ],
        "phpmd": "\"vendor/bin/phpmd\" autoload.php,interface,class,script,test,test_case text cleancode,codesize,controversial,design,naming,unusedcode",
        "phpcs": "\"vendor/bin/phpcs\" autoload.php interface class script test test_case --standard=PSR2",
        "phpcbf": "\"vendor/bin/phpcbf\" autoload.php interface class script test test_case --standard=PSR2",
        "phpunit": "CodeMommy\\RoutePHP\\Script\\PHPUnit::start",
        "clean": "CodeMommy\\RoutePHP\\Script\\Clean::start"
    }
}