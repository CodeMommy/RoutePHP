{
    "name": "codemommy/routephp",
    "type": "library",
    "license": "Apache-2.0",
    "homepage": "http://www.codemommy.com",
    "description": "Route for MVC framework",
    "keywords": [
        "CodeMommy",
        "RoutePHP",
        "Route",
        "PHP"
    ],
    "support": {
        "issues": "https://github.com/CodeMommy/RoutePHP/issues",
        "source": "https://github.com/CodeMommy/RoutePHP"
    },
    "authors": [
        {
            "name": "Candison November",
            "role": "Founder",
            "email": "kandisheng@163.com",
            "homepage": "http://www.kandisheng.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CodeMommy\\RoutePHP\\": [
                "source/",
                "interface/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeMommy\\RoutePHP\\Script\\": "script/",
            "Controller\\": "test/case/controller/",
            "ControllerNoNamespaceRoot\\": "test/case/"
        }
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/http-foundation": "2.8.*",
        "symfony/routing": "2.8.*"
    },
    "require-dev": {
        "CodeMommy/TaskPHP": "*",
        "phpunit/phpunit": "*",
        "phpmd/phpmd": "*",
        "squizlabs/php_codesniffer": "*"
    },
    "scripts": {
        "environment": [
            "composer -V",
            "php -v",
            "php -m"
        ],
        "update-project": [
            "git pull",
            "composer self-update",
            "composer update"
        ],
        "test": [
            "composer phpunit",
            "composer phpcs",
            "composer phpmd"
        ],
        "phpcs": [
            "\"vendor/bin/phpcs\" interface --standard=PSR2",
            "\"vendor/bin/phpcs\" source --standard=PSR2",
            "\"vendor/bin/phpcs\" script --standard=PSR2",
            "\"vendor/bin/phpcs\" test --standard=PSR2"
        ],
        "phpmd": "\"vendor/bin/phpmd\" interface,source,script,test text cleancode,codesize,controversial,design,naming,unusedcode",
        "phpunit": "CodeMommy\\RoutePHP\\Script\\PHPUnit::start",
        "clean": "CodeMommy\\RoutePHP\\Script\\Clean::start"
    }
}